{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
  "rules": {
    "import/prefer-default-export": "off",
    "class-methods-use-this": [0],
    "lines-between-class-members": [0],
    "import/extensions": ["error", {
      "js": "ignorePackages"
      }
    ]
  }
}


