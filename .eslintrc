{
  "root": true,
  "extends": ["airbnb-base", "prettier"],
  "overrides": [
    {
      "files": "__tests__/**/*",
      "env": {
        "jest": true
      }
    }
  ],
  "plugins": ["prettier"],
  "rules": {
    "strict": [0, "global"],
    "class-methods-use-this": [0],
    "no-param-reassign": [0]
  }
}
